<%- include ('header.ejs') %>
    
<div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Redimir Promo !!!</span>
          <p><strong>Token:</strong>  <%= userId._id %></p>
          <p><strong>Fecha de compra:</strong>  <%= userId.createdAt %></p>
          <p><strong>Correo:</strong>  <%= userId.correo %></p>
          <p><strong>Telefono:</strong>  <%= userId.telefono %></p>
        </div>
        <div class="card-action">
          <a class="waves-effect waves-light btn modal-trigger" href="#modal">Modal</a>
          <a class="waves-effect waves-light btn modal-trigger" href="http://app.remicos.com.co:84">cancelar</a>
        </div>
      </div>
    </div>
  </div>

  

  <!-- Modal Structure -->
  <div id="modal" class="modal bottom-sheet">
    <div class="modal-content">
      <h4>Aprobaci√≥n</h4>
      <p>Por favor ingrese el codigo de aprobacion para redimir la Promo</p>
    </div>
    <div class="modal-footer">
      <a href="#!" class="modal-close waves-effect waves-green btn-flat">Aprobar</a>
    </div>
  </div>

  <script>
    $(document).ready(function(){
    $('.modal').modal();
    })
  </script>

        


